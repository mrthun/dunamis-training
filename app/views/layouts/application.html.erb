<!DOCTYPE html>
<html>
  <head>
    <title>DUNAMIS</title>
    <%= stylesheet_link_tag "fullcalendar" %>
    <%= stylesheet_link_tag "fullcalendar.print", :media => 'print' %>
    
    <%= javascript_include_tag :defaults %>
    <%= javascript_include_tag "jquery-1.4.4.min","jquery-ui-1.8.6.custom.min","jquery.form","fullcalendar.min" %>
    <%= csrf_meta_tag %>
  </head>
  <body>
    <%= render :partial => "/shared/header" %>
    <% flash_messages %>
    <%= yield %>
    <%= render :partial => "/shared/footer" %>
  </body>
</html>
