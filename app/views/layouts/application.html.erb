<!DOCTYPE html>
<html>
  <head>
    <title>DUNAMIS</title>
    <%= stylesheet_link_tag "fullcalendar" %>
    <%= stylesheet_link_tag "fullcalendar.print", :media => 'print' %>
    <%= stylesheet_link_tag "style" %>
    <%= javascript_include_tag :defaults %>
    <%= javascript_include_tag "jquery","jquery-1.4.4.min","jquery-ui-1.8.6.custom.min","jquery.form","fullcalendar.min" %>
    <%= csrf_meta_tag %>
  </head>
  <body>
    <div class="wrrapper">
      <div class="wrrapperBgHolder">
        <%= render :partial => "/shared/header" %>
        <%= render :partial => "/shared/tabs" %>

        <div class="contentHolder">
          <% flash_messages %>
          <div class="container">
            <%= yield %>
          </div>
        </div>
        <%= render :partial => "/shared/footer" %>
      </div>
    </div>
  </body>
</html>
